{
  "name": "dhealth-utils",
  "version": "0.2.0",
  "description": "Utility library for dHealth",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' nyc mocha -r ts-node/register 'test/**/*.ts' && nyc report --reporter=text-lcov | coveralls",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:dHealth-Symbol-Lab/dhealth-utils.git"
  },
  "keywords": [
    "dHealth",
    "symbol",
    "utils",
    "library",
    "package",
    "client"
  ],
  "author": "Son Nguyen",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "homepage": "https://github.com/dHealth-Symbol-Lab/dhealth-utils.git",
  "devDependencies": {
    "@types/bip32": "^1.0.2",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.14.37",
    "@types/sinon": "^10.0.3",
    "bip32": "^1.0.4",
    "chai": "^4.3.4",
    "coveralls": "^3.1.1",
    "mocha": "^9.1.1",
    "nyc": "^15.1.0",
    "sinon": "^11.1.2",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "rxjs": "^6.6.7",
    "symbol-hd-wallets": "^0.14.1",
    "symbol-sdk": "^1.0.0"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "lib/*",
      "./*",
      "coverage/*",
      "test/*"
    ],
    "reporter": [
      "text"
    ],
    "all": true
  }
}
